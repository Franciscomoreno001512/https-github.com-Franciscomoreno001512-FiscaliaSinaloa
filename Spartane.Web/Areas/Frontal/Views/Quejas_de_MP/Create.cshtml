<script type="text/javascript">


   function GetCatalogPopup(catalog, nameAttribute, isMR, nameMR) {
        if (!isMR)
            var url = "@Url.Action("Create", "Catalog", new { Module_Id = "1", isPartial = true, nameAttribute = "AttributoNombre" })";
        else
            var url = "@Url.Action("Create", "Catalog", new { Module_Id = "1", isPartial = true, isMR = true, nameMR = "Multirenglon",nameAttribute= "AttributoNombre" })";

        url = url.replace("Catalog", catalog).replace("Multirenglon", nameMR).replace("AttributoNombre", nameAttribute);

        window.open(htmlDecode(url), '_blank', 'left=200,top=100,width=850,height=550,toolbar=0,resizable=0');
    }

    function RefreshCatalog(catalog, control) {

	 if ($('#' + control).hasClass('AutoComplete'))   {
		   if ($('#' + control).attr("data-isfilter") == "true") {
				eval(GetReglaFilter(control, $('#ObjectId').val()));
				return;
			}
		}
			
			var url = '@Url.Content("~/Frontal/Quejas_de_MP/GetCatalogAll")';
			url = url.replace(/Catalog/g, catalog);

			$.ajax({
				url: url,
				async: true,
				success:function(result)
				{
					addItemsToSelect($('#' + control), result);
				},
				error: function (result)
				{
					showNotification("@Resources.Resources.ErrorContactingServer", "error");
				}
			});
		
    }


        function SendQuejas_de_MPData(callBack) {
			
					
		 var arr = []; 
		 $("form#CreateQuejas_de_MP").find(':input').each(function( index ) {		 
			if ($(this).prop('disabled')==true|| $(this).prop('disabled')=="disabled")
			{		
				arr.push($(this).attr('id')); 
				$(this).prop('disabled', false);  	
			}
	    });
		
             var formData = new FormData($("form#CreateQuejas_de_MP")[0]);
			$.each( arr, function( key, value ) {
			  $('#' +value).prop('disabled', true);
			});
			var id = $("#ReferenceClave").val();
            var isNew = $('#hdnIsNew').val();
           
			if (Tab_Datos_Generales && !checkClientValidate('Quejas_de_MPCreateForm', 'Datos_Generales')) {
				return false;
			}						

			if (Tab_Datos_de_Media_Filiacion && !checkClientValidate('Quejas_de_MPCreateForm', 'Datos_de_Media_Filiacion')) {
				return false;
			}						

			if (Tab_Datos_de_los_Hechos && !checkClientValidate('Quejas_de_MPCreateForm', 'Datos_de_los_Hechos')) {
				return false;
			}						

			if (Tab_Datos_de_Identificacion && !checkClientValidate('Quejas_de_MPCreateForm', 'Datos_de_Identificacion')) {
				return false;
			}						

			if (Tab_Datos_de_Domicilio && !checkClientValidate('Quejas_de_MPCreateForm', 'Datos_de_Domicilio')) {
				return false;
			}						


			
			if (isNew == "True")
            {
                id = CreateID();
            }
			
			
			if (Tab_Datos_Generales)
				Save_Datos_Generales(id);

			if (Tab_Datos_de_Media_Filiacion)
				Save_Datos_de_Media_Filiacion(id);

			if (Tab_Datos_de_los_Hechos)
				Save_Datos_de_los_Hechos(id);

			if (Tab_Datos_de_Identificacion)
				Save_Datos_de_Identificacion(id);

			if (Tab_Datos_de_Domicilio)
				Save_Datos_de_Domicilio(id);


			showNotification("@Resources.Resources.SaveSuccess", "success");
			callBack(id);
            return true;
        };
		function CreateID()
		{
			var newId = 0;
			$.ajax({
				url: '@Url.Content("~/Frontal/Quejas_de_MP/CreateID")',
				type: 'GET',
				async: false,
				success: function (result) {
					if (result < 0) {
						showNotification("@Resources.Resources.ErrorSave", "error");
					} else {
						newId = result;
					}
				},
				error: function (result) {
					showNotification("@Resources.Resources.ErrorContactingServer", "error");
				},
				cache: false,
				contentType: false,
				processData: false
			});
			return newId;
		}
		
