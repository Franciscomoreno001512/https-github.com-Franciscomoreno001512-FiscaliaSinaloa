        function getdetalle_de_observaciones_mpiData() {
            $.ajax({
                url: "@Url.Content("~/Frontal/expediente_ministerio_publico/Getdetalle_de_observaciones_mpi")" + "?draw=1&start=1&length=2147483647" + ($("#Referenceclave").val() != "" ? "&RelationId=" + $("#Referenceclave").val() : ""),
                type: 'GET',
                dataType: "json",
                async: false,
                success: function (result) {
                    detalle_de_observaciones_mpiDataDataTable = result.data;
                    showdetalle_de_observaciones_mpiGrid(result.data);
                    $('#detalle_de_observaciones_mpiGrid .inputMoney').inputmask("currency", { prefix: "$", rightAlign: false });
                    $('#detalle_de_observaciones_mpiGrid .gridDatePicker').attr("readonly", "");
                },
                error: function (result) {
                    showNotification("@Resources.Resources.ErrorContactingServer", "error");
                }
            });
        }
        function detalle_de_observaciones_mpiGetUpdateRowControls(data, nameOfTable, rowIndex) {

            var columnData = [];
            var counter = 0;
            var inputData = "<input type='text' class='fullWidth form-control' />";

            columnData[counter++] = $($($.parseHTML(inputData)).val(data.observaciones)).addClass('observaciones').attr('id', nameOfTable + 'observaciones' + rowIndex).attr('data-field', 'observaciones');
            columnData[counter++] = $($($.parseHTML(GetGridDatePicker())).val(data.fecha)).addClass('fecha').attr('id', nameOfTable + 'fecha' + rowIndex).attr('data-field', 'fecha');
            columnData[counter++] = $($($.parseHTML(GetGridTimePicker())).val(data.hora)).addClass('hora').attr('id', nameOfTable + 'hora' + rowIndex).attr('data-field', 'hora');
            columnData[counter++] = $($($.parseHTML(GetGridAutoComplete(data.usuario_que_realiza_observacionName != undefined ? data.usuario_que_realiza_observacionName : null, 'AutoComplete' + nameOfTable + 'usuario_que_realiza_observacion', data.usuario_que_realiza_observacion)))).addClass('usuario_que_realiza_observacion').attr('id', nameOfTable + 'usuario_que_realiza_observacion' + rowIndex).attr('data-field', 'usuario_que_realiza_observacion').after($.parseHTML(addNew('detalle_de_observaciones_mpi', 'Spartan_User','usuario_que_realiza_observacion', 267117)));

            return columnData;
        }



        function GetFileInfo(fileData, fileUploadData) {

            var $div = $("<div>");
            if (fileUploadData != null && fileUploadData.files.length > 0) {

                $div.append("<label>" + fileUploadData.files[0].name + "</label>");
                //<i class="fa fa-times-circle" id='removeFileAttachment'>
                $div.append($("<i class='fa fa-times-circle' id='removeFileAttachment'>").click(function () {
                    if (confirm(htmlDecode("@Resources.Resources.DeleteConfirm"))) {
                        var parent = $(this).parent().parent();
                        parent.empty();
