@using System
@using System.Collections.Generic
@using System.Globalization
@using Spartane.Core.Domain.Permission
@using Spartane.Core.Domain.Spartane_File
@using Spartane.Web.Helpers
@model Spartane.Web.Areas.Frontal.Models.Registro_de_UsuarioModel
@{

    /*

    */


    Permission permission = ViewBag.Permission;

    if (ViewBag.IsPartial || ViewBag.viewInEframe)
    {
        Layout = "~/Areas/Frontal/Views/Shared/_LayoutPopup.cshtml";
    }
}
@*fjmore portal*@
<style>
    #page-wrapper {
        margin: 0px !important;
    }
</style>
<div class="loading style-2"><div class="loading-wheel"></div></div>
<div class="modal fade in" id="dvAttributeValue" style="background-color:rgba(0, 0, 0, 0.7);">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-body">
                <div class="form-group">
                    <label class="col-sm-3 control-label" id="lblAttributeType">@Resources.Resources.Value</label>
                    <div class="col-sm-7">
                        <input type="text" title="Value" class="form-control" id="txtAttributeValue" />
                    </div>
                    <div class="col-sm-2">
                        <a class="btn btn-primary" onclick="SaveValue()">@Resources.Resources.Submit</a>
                    </div>
                    <input type="hidden" id="hdnAttributType" />
                    <input type="hidden" id="hdnAttributElement" />
                    <input type="hidden" id="hdnAttributNumber" />
                    <input type="hidden" id="hdnElementType" />
                </div>
            </div>
        </div>
    </div>
</div>

<div class="row wrapper border-bottom white-bg page-heading portal-c">

    <div class="row">
        <div class="col-lg-10">
            <h2>
                @Resources.Registro_de_UsuarioResources.Registro_de_Usuario
                <button onclick="Registro_de_UsuarioGetBusinessRule('', 'Al Abrir Pantalla', '0')" type="button" class="btn btn-info btn-lg displayBusinessProperyButton displayRuleButton" data-toggle="modal" data-target="#Registro_de_UsuarioBusinessRule-form">
                    <a class="top" title="" data-placement="right" data-toggle="tooltip" href="#" data-original-title="@Resources.Resources.BusinessRuleToBegin">
                        <i class="fa fa-cogs fa-spin"></i>
                    </a>
                </button>
            </h2>
            @*<ol class="breadcrumb">
                    <li>
                        <a href="@Url.Action(" Index", "Home" )">@Resources.Resources.Home</a>
                    </li>
                    <li class="active">
                        @if (Session["PhaseName"] != null)
                        {
                        <strong>@Session["PhaseName"].ToString()</strong>
                        }
                        else
                        {
                        <strong>@Resources.Registro_de_UsuarioResources.Registro_de_Usuario</strong>
                        }

                    </li>
                    <li class="active">
                        <strong>@Resources.Resources.Create</strong>
                    </li>
                </ol>*@
        </div>
        <div class="col-lg-2">

            @{
                if (Convert.ToInt32(Spartane.Web.SessionHelper.Role) == (int)Spartane.Core.Enums.SpartanRoleEnum.Admin)
                {
                    <h2>&nbsp;</h2>
                    <button onclick="Registro_de_UsuarioDisplayBusinessRule()" type="button" class="btn btn-info btn-lg displayBusinessProperyButton">
                        <i class="fa fa-cogs fa-spin"></i>
                    </button>

                    <div aria-hidden="true" class="modal fade in Registro_de_UsuarioPropertyBusinessModal" id="Registro_de_UsuarioPropertyBusinessModal-form" style="display: none; background-color:rgba(0, 0, 0, 0.7);">
                        <div class="modal-dialog">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h4 class="modal-title">
                                        @Resources.Resources.titleBussinesRules
                                        <button data-dismiss="modal" class="close" type="button"><span aria-hidden="true">×</span><span class="sr-only">@Resources.Resources.Close</span></button>
                                    </h4>
                                </div>
                                <div class="modal-body">
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <a class="btn btn-primary full-width" data-dismiss="modal" data-toggle="modal" data-target="#PropertyModal-form" onclick="GetPropertyBag()">@Resources.Resources.Property</a>
                                        </div>
                                        <div class="clearfix col-sm-12">&nbsp;</div>
                                        <div class="col-sm-12">
                                            <a class="btn btn-primary full-width" data-dismiss="modal" data-toggle="modal" data-target="#Registro_de_UsuarioBusinessRule-form" onclick="Registro_de_UsuarioGetBusinessRule()">Business Rule</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div aria-hidden="true" data-backdrop="static" class="modal fade in PropertyModal" id="PropertyModal-form" style="display: none; background-color:rgba(0, 0, 0, 0.7);">
                    </div>
                    <div aria-hidden="true" data-backdrop="static" class="modal fade in Registro_de_UsuarioBusinessRuleModal" id="Registro_de_UsuarioBusinessRule-form" style="display: none; background-color:rgba(0, 0, 0, 0.7);">
                    </div>


                }
            }

        </div>
    </div>

    <div class="wrapper wrapper-content animated fadeInRight">
        <div class="ibox float-e-margins">
            <div class="ibox-content">
                @using (Html.BeginForm("Post", "Registro_de_Usuario", FormMethod.Post, new { id = "CreateRegistro_de_Usuario", @readonly = "true", @class = "form-horizontal", enctype = "multipart/form-data" }))
                {
                    <input type="hidden" id="hdnIsNew" name="hdnIsNew" value="@ViewBag.IsNew.ToString()" />
                    <span id="popupNotification"></span>
                    <div class="container-fluid Registro_de_UsuarioCreateForm" id="viewmodeledit">
                        <div class="alert alert-warning" id="controlsRequerid" style="display:none">
                            <a href="#" class="close" onclick="$('#controlsRequerid').hide();">&times;</a>
                            <span id="textRequired"></span>
                        </div>
                        <div class="tabs-container">
                            <ul class="nav nav-tabs">
                                <li class="active" data-fase="0"><a data-toggle="tab" href="#tabDatos_Generales"> @Resources.Registro_de_UsuarioResources.TabDatos_Generales</a></li>
                                <li class="" data-fase="1"><a data-toggle="tab" href="#tabDatos_para_Iniciar_Sesion"> @Resources.Registro_de_UsuarioResources.TabDatos_para_Iniciar_Sesion</a></li>
                                <li class="" data-fase="2"><a data-toggle="tab" href="#tabDatos_para_validar_tu_cuenta"> @Resources.Registro_de_UsuarioResources.TabDatos_para_validar_tu_cuenta</a></li>
                                <li class="" data-fase="3"><a data-toggle="tab" href="#tabValidacion">Verificación</a></li>
                                <li class="" data-fase="4"><a data-toggle="tab" href="#tabLogica"> @Resources.Registro_de_UsuarioResources.TabLogica</a></li>

                            </ul>
                            <div class="tab-content">
                                <div id="tabDatos_Generales" class="tab-pane active">
                                    <div class="panel-body">
                                        <div class="form-group" id="divClave">
                                            <label class="col-sm-2 control-label">@Resources.Registro_de_UsuarioResources.Clave</label>
                                            <div class="col-sm-8" data-field-id="Clave" data-attribute="269001" data-field-name="@Resources.Registro_de_UsuarioResources.Clave">
                                                <label class="form-control" for="Clave" id="lblClave">
                                                    @if (Model.Clave == 0)
                                                    {
                                                        <text>@Resources.Resources.ClaveDefault</text>
                                                    }
                                                    else
                                                    {
                                                        <text>@Model.Clave</text>
                                                    }
                                                </label>
                                            </div>
                                        </div>
                                        <div class="form-group" id="divNombres">
                                            <label class="col-sm-2 control-label">@Resources.Registro_de_UsuarioResources.Nombres</label>
                                            <div class="col-sm-8" data-field-id="Nombres" data-attribute="269002" data-field-name="@Resources.Registro_de_UsuarioResources.Nombres">
                                                <input class="form-control Datos_Generales" id="Nombres" name="Nombres" type="text" value="@Model.Nombres">
                                                @*<span class="field-validation-valid" data-valmsg-for="Nombres" data-valmsg-replace="true"></span>*@
                                            </div>
                                        </div>
                                        <div class="form-group" id="divApellido_Paterno">
                                            <label class="col-sm-2 control-label">@Resources.Registro_de_UsuarioResources.Apellido_Paterno</label>
                                            <div class="col-sm-8" data-field-id="Apellido_Paterno" data-attribute="269003" data-field-name="@Resources.Registro_de_UsuarioResources.Apellido_Paterno">
                                                <input class="form-control Datos_Generales" id="Apellido_Paterno" name="Apellido_Paterno" type="text" value="@Model.Apellido_Paterno">
                                                @*<span class="field-validation-valid" data-valmsg-for="Apellido_Paterno" data-valmsg-replace="true"></span>*@
                                            </div>
                                        </div>
                                        <div class="form-group" id="divApellido_Materno">
                                            <label class="col-sm-2 control-label">@Resources.Registro_de_UsuarioResources.Apellido_Materno</label>
                                            <div class="col-sm-8" data-field-id="Apellido_Materno" data-attribute="269004" data-field-name="@Resources.Registro_de_UsuarioResources.Apellido_Materno">
                                                <input class="form-control Datos_Generales" id="Apellido_Materno" name="Apellido_Materno" type="text" value="@Model.Apellido_Materno">
                                                @*<span class="field-validation-valid" data-valmsg-for="Apellido_Materno" data-valmsg-replace="true"></span>*@
                                            </div>
                                        </div>
                                        <div class="form-group" id="divNombre_Completo">
                                            <label class="col-sm-2 control-label">@Resources.Registro_de_UsuarioResources.Nombre_Completo</label>
                                            <div class="col-sm-8" data-field-id="Nombre_Completo" data-attribute="269005" data-field-name="@Resources.Registro_de_UsuarioResources.Nombre_Completo">
                                                <textarea id="Nombre_Completo" name="Nombre_Completo" class="form-control Datos_Generales" rows="4" cols="50">@Model.Nombre_Completo</textarea>
                                                @*<span class="field-validation-valid" data-valmsg-for="Nombre_Completo" data-valmsg-replace="true"></span>*@
                                            </div>
                                        </div>
                                    </div>
                                </div>


                                <div id="tabDatos_para_Iniciar_Sesion" class="tab-pane">
                                    <div class="panel-body">
                                        <label style="font-size:17px;">Es necesario introducir una contraseña segura, mínimo 8 caracteres, con una letra minúscula, una letra mayúscula, un número y un carácter especial.</label>
                                        <div class="form-group" id="divUsuario">
                                            <label class="col-sm-2 control-label">@Resources.Registro_de_UsuarioResources.Usuario</label>
                                            <div class="col-sm-8" data-field-id="Usuario" data-attribute="269006" data-field-name="@Resources.Registro_de_UsuarioResources.Usuario">
                                                <input class="form-control Datos_para_Iniciar_Sesion" id="Usuario" name="Usuario" type="text" value="@Model.Usuario">
                                                @*<span class="field-validation-valid" data-valmsg-for="Usuario" data-valmsg-replace="true"></span>*@
                                            </div>
                                        </div>
                                        <div class="form-group" id="divContrasena">
                                            <label class="col-sm-2 control-label">@Resources.Registro_de_UsuarioResources.Contrasena</label>
                                            <div class="col-sm-8" data-field-id="Contrasena" data-attribute="269007" data-field-name="@Resources.Registro_de_UsuarioResources.Contrasena">
                                                <input class="form-control Datos_para_Iniciar_Sesion" maxlength="10" id="Contrasena" name="Contrasena" type="password" value="@Model.Contrasena">
                                                @*<span class="field-validation-valid" data-valmsg-for="Contrasena" data-valmsg-replace="true"></span>*@
                                            </div>
                                        </div>
                                        <div class="form-group" id="divConfirmar_Contrasena">
                                            <label class="col-sm-2 control-label">@Resources.Registro_de_UsuarioResources.Confirmar_Contrasena</label>
                                            <div class="col-sm-8" data-field-id="Confirmar_Contrasena" data-attribute="269008" data-field-name="@Resources.Registro_de_UsuarioResources.Confirmar_Contrasena">
                                                <input class="form-control Datos_para_Iniciar_Sesion" maxlength="10" id="Confirmar_Contrasena" name="Confirmar_Contrasena" type="password" value="@Model.Confirmar_Contrasena">
                                                @*<span class="field-validation-valid" data-valmsg-for="Confirmar_Contrasena" data-valmsg-replace="true"></span>*@
                                            </div>
                                        </div>
                                    </div>
                                </div>


                                <div id="tabDatos_para_validar_tu_cuenta" class="tab-pane">
                                    <div class="panel-body">
                                        <label>Es necesario que para efectos de autenticación proporciones tu correo electrónico o tu teléfono celular, o ambos.</label>
                                        <div class="form-group" id="divCorreo">
                                            <label class="col-sm-2 control-label">@Resources.Registro_de_UsuarioResources.Correo</label>
                                            <div class="col-sm-8" data-field-id="Correo" data-attribute="269009"
                                                 data-field-name="@Resources.Registro_de_UsuarioResources.Correo">
                                                <input class="form-control Datos_para_validar_tu_cuenta" id="Correo" name="Correo" type="text"
                                                       value="@Model.Correo">
                                                @*<span class="field-validation-valid" data-valmsg-for="Correo" data-valmsg-replace="true"></span>*@
                                            </div>
                                        </div>
                                        <div class="form-group" id="divCelular">
                                            <label class="col-sm-2 control-label">@Resources.Registro_de_UsuarioResources.Celular</label>
                                            <div class="col-sm-8" data-field-id="Celular" data-attribute="269010"
                                                 data-field-name="@Resources.Registro_de_UsuarioResources.Celular">
                                                <input class="form-control Datos_para_validar_tu_cuenta" id="Celular" name="Celular" type="text"
                                                       value="@Model.Celular">
                                                @*<span class="field-validation-valid" data-valmsg-for="Celular" data-valmsg-replace="true"></span>*@
                                            </div>
                                        </div>
                                        <div class="checkbox">
                                            <input id="idusopersonal" type="checkbox">
                                            <label>Confirmo que el número celular es de uso personal y no de un tercero.</label>
                                        </div>
                                        <div class="checkbox">
                                            <input id="idterminos" type="checkbox">
                                            <label> Confirmo que he leído y entiendo <a href="#">los términos y condiciones  de uso del sitio</a> y <a href="#">el aviso de privacidad de datos</a>.</label>
                                        </div>
                                    </div>
                                </div>

                                <div id="tabValidacion" class="tab-pane">
                                    <div class="panel-body">
                                        <label>Para poder verificar su cuenta por favor introduzca el código de verificación de 9 dígitos que le enviamos por SMS a su teléfono y/o a su cuenta de correo electrónico.</label>
                                        <div class="form-group" id="divCodigo_Verificacion_Correo">
                                            <label class="col-sm-2 control-label">@Resources.Registro_de_UsuarioResources.Codigo_Verificacion_Correo</label>
                                            <div class="col-sm-8" data-field-id="Codigo_Verificacion_Correo" data-attribute="269011"
                                                 data-field-name="@Resources.Registro_de_UsuarioResources.Codigo_Verificacion_Correo">
                                                <input class="form-control Validacion" id="Codigo_Verificacion_Correo" name="Codigo_Verificacion_Correo"
                                                       type="text" value="@Model.Codigo_Verificacion_Correo">
                                                <span>
                                                    ¿No recibiste el codigo? <a href="#" id="idReenviarCodigoCorreo">Reenviar Código al Correo</a>
                                                    @*<span class="field-validation-valid" data-valmsg-for="Codigo_Verificacion_Correo"
                                                        data-valmsg-replace="true"></span>*@

                                            </div>
                                        </div>
                                        <div class="form-group" id="divCodigo_Verificacion_Celular">
                                            <label class="col-sm-2 control-label">@Resources.Registro_de_UsuarioResources.Codigo_Verificacion_Celular</label>
                                            <div class="col-sm-8" data-field-id="Codigo_Verificacion_Celular" data-attribute="269012"
                                                 data-field-name="@Resources.Registro_de_UsuarioResources.Codigo_Verificacion_Celular">
                                                <input class="form-control Validacion" id="Codigo_Verificacion_Celular"
                                                       name="Codigo_Verificacion_Celular" type="text" value="@Model.Codigo_Verificacion_Celular">
                                                <span>
                                                    ¿No recibiste el codigo? <a href="#" id="idReenviarCodigoCelular">Reenviar Código al Celular</a>
                                                    @*<span class="field-validation-valid" data-valmsg-for="Codigo_Verificacion_Celular"
                                                        data-valmsg-replace="true"></span>*@
                                            </div>
                                        </div>
                                        <div class="form-group" id="divCodigo_Reestablecer_Contrasena">
                                            <label class="col-sm-2 control-label">@Resources.Registro_de_UsuarioResources.Codigo_Reestablecer_Contrasena</label>
                                            <div class="col-sm-8" data-field-id="Codigo_Reestablecer_Contrasena" data-attribute="269013"
                                                 data-field-name="@Resources.Registro_de_UsuarioResources.Codigo_Reestablecer_Contrasena">
                                                <input class="form-control Validacion" id="Codigo_Reestablecer_Contrasena"
                                                       name="Codigo_Reestablecer_Contrasena" type="text" value="@Model.Codigo_Reestablecer_Contrasena">
                                                @*<span class="field-validation-valid" data-valmsg-for="Codigo_Reestablecer_Contrasena"
                                                    data-valmsg-replace="true"></span>*@
                                            </div>
                                        </div>
                                    </div>
                                </div>


                                <div id="tabLogica" class="tab-pane">
                                    <div class="panel-body">
                                        <div class="form-group" id="divEl_correo_fue_Validado">
                                            <label class="col-sm-2 control-label">@Resources.Registro_de_UsuarioResources.El_correo_fue_Validado</label>
                                            <div class="col-sm-8" data-field-id="El_correo_fue_Validado" data-attribute="269014" data-field-name="@Resources.Registro_de_UsuarioResources.El_correo_fue_Validado">
                                                @Html.CheckBoxFor(m => m.El_correo_fue_Validado, new { @class = "i-checks Logica" })
                                            </div>
                                        </div>
                                        <div class="form-group" id="divEl_Celular_fue_Validado">
                                            <label class="col-sm-2 control-label">@Resources.Registro_de_UsuarioResources.El_Celular_fue_Validado</label>
                                            <div class="col-sm-8" data-field-id="El_Celular_fue_Validado" data-attribute="269015" data-field-name="@Resources.Registro_de_UsuarioResources.El_Celular_fue_Validado">
                                                @Html.CheckBoxFor(m => m.El_Celular_fue_Validado, new { @class = "i-checks Logica" })
                                            </div>
                                        </div>
                                    </div>
                                </div>

                            </div>
                            <br />
                            <div class="form-actions fluid">
                                <input type="hidden" value="@Model.Clave" id="ReferenceClave" />
                                <input type="hidden" value="@Model.Clave" name="Clave" id="ClaveId" />
                                <input type="hidden" value="@ViewBag.Operation" name="Operation" id="Operation" />
                                @if (!ViewBag.Consult)
                                {
                                    <input type="button" value="@Resources.Resources.Guardar" id="Registro_de_UsuarioGuardar" class="btn btn-w-m btn-primary btn_save" onclick="return checkClientValidate('Registro_de_UsuarioCreateForm');" />
                                    <button onclick="Registro_de_UsuarioGetBusinessRule('', 'Al Guardar o al Cerrar la Pantalla', '2')" type="button" class="btn btn-info btn-lg displayBusinessProperyButton displayRuleButton" data-toggle="modal" data-target="#Registro_de_UsuarioBusinessRule-form">
                                        <a class="top" title="" data-placement="right" data-toggle="tooltip" href="#" data-original-title="@Resources.Resources.BusinessRuleBeforeSave">
                                            <i class="fa fa-cogs fa-spin"></i>
                                        </a>
                                    </button>

                                    <input type="button" value="@Resources.Resources.GuardarNuevo" id="Registro_de_UsuarioGuardarYNuevo" onclick="return checkClientValidate('Registro_de_UsuarioCreateForm');" data-bind="    click: saveAndNew" class="btn btn-w-m btn-success btn_saveandnew" />
                                    <input type="button" value="@Resources.Resources.GuardarCopia" id="Registro_de_UsuarioGuardarYCopia" onclick="return checkClientValidate('Registro_de_UsuarioCreateForm');" data-bind="    click: saveAndNew" class="btn btn-w-m btn-success btn_saveandcopy" />
                                    if (permission.Configure)
                                    {
                                        <a title="@Resources.Resources.Configure" id="ConfigureSave" name="ConfigureSave" onclick="SaveElementAttributes()" class="btn btn-success btn-configure">@Resources.Resources.Configure</a>
                                    }
                                }

                                <input type="hidden" id="hdnConfigureType" />
                                <input type="hidden" id="hdnRegistro_de_UsuarioBusinessRuleFieldId" />
                                <input type="hidden" id="hdnRegistro_de_UsuarioBusinessRuleFieldName" />
                                <input type="hidden" id="hdnRegistro_de_UsuarioBusinessRuleAttribute" />

                                <div class="row">
                                    <div class="col-xs-10 col-xs-offset-1">
                                        <div class="row">

                                            <input type="button" value="@Resources.Resources.Cancelar" id="Registro_de_UsuarioCancelar" name="Registro_de_UsuarioCancelar" class="btn btn-danger noConsult btn_cancelar" data-bind="click: cancel" tabindex="16" />
                                            <input type="button" value="Anterior" id="Registro_de_UsuarioAnterior" class="btn btn-w-m btn-primary btn_save" />
                                            <input type="button" value="Siguiente" id="Registro_de_UsuarioSiguiente" class="btn btn-w-m btn-primary btn_save" />
                                            <input type="button" value="Reenviar" id="Registro_de_UsuarioReenviar" class="btn btn-w-m btn-primary btn_save" />
                                        </div>
                                    </div>
                                </div>
                                <button onclick="Registro_de_UsuarioGetBusinessRule('', 'Al Cancelar', '4')" type="button" class="btn btn-info btn-lg displayBusinessProperyButton displayRuleButton" data-toggle="modal" data-target="#Registro_de_UsuarioBusinessRule-form">
                                    <a class="top" title="" data-placement="right" data-toggle="tooltip" href="#" data-original-title="@Resources.Resources.BusinessRuleToBegin">
                                        <i class="fa fa-cogs fa-spin"></i>
                                    </a>
                                </button>
                            </div>
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
    <input id="hdnApplicationDirectory" type="hidden" value="@Url.Content(" ~/")" />
    <div id="divmodalaux" class="hidden" style="border: 2px solid lightgray; border-radius: 4px; z-index: 99999; position: absolute; width: 1800px; height: 1200px; background: rgba(192,192,192,0.5)!important; top: -300px; left: -300px;">
        <div id="modalaux" style="top:400px;left:-100px;" class="modal-dialog">
            <div class="modal-content">
                <div class="modal-body">
                    <div class="row">
                        <div class="col-4">
                            <img class="logo-fiscalia logo-modal" src="~/Images/logotipos/fiscalia200px.png" alt="Logotipo Fiscalia Sinaloa">
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-sm-12 b-r">
                            <h2 id="modalauxtit" class="m-t-none m-b text-capitalize">Aviso Importante</h2>
                            <p style="font-size:16px;" id="modalauxtxt">Parrafo con infomación</p>

                            <!-- DIV DE ERROR. Solo si se requiere-->
                            <div class="form-group">
                                <div id="error-old-password"></div>
                            </div>
                            <!-- FIN: DIV DE ERROR-->
                            <!-- DIV DE BOTONES DE ACEPTAR Y CANCELAR. Solo si se requiere-->
                            <div style="text-align:right;" class="form-group">
                                <button id="saveaux" type="button" class="btn btn-w-m btn-primary btn_save">Aceptar</button>
                            </div>
                            <!-- FIN: DIV DE BOTONES DE GUARDAD/ACEPTAR Y CANCELAR-->

                        </div>
                    </div>
                </div>
            </div>
