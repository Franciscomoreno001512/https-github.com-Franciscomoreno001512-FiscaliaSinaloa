        function GetCatalogPopup(catalog, nameAttribute, isMR, nameMR) {
            if (!isMR)
                var url = "@Url.Action("Create", "Catalog", new { Module_Id = "1", isPartial = true, nameAttribute = "AttributoNombre" })";
            else
            var url = "@Url.Action("Create", "Catalog", new { Module_Id = "1", isPartial = true, isMR = true, nameMR = "Multirenglon",nameAttribute= "AttributoNombre" })";

            url = url.replace("Catalog", catalog).replace("Multirenglon", nameMR).replace("AttributoNombre", nameAttribute);

            window.open(htmlDecode(url), '_blank', 'left=200,top=100,width=850,height=550,toolbar=0,resizable=0');
        }

        function RefreshCatalog(catalog, control) {

            if ($('#' + control).hasClass('AutoComplete')) {
                if ($('#' + control).attr("data-isfilter") == "true") {
                    eval(GetReglaFilter(control, $('#ObjectId').val()));
                    return;
                }
            }

            var url = '@Url.Content("~/Frontal/Registro_de_Usuario/GetCatalogAll")';
            url = url.replace(/Catalog/g, catalog);

            $.ajax({
                url: url,
                async: true,
                success: function (result) {
                    addItemsToSelect($('#' + control), result);
                },
                error: function (result) {
                    showNotification("@Resources.Resources.ErrorContactingServer", "error");
                }
            });

        }

        var NuevoIDV = 0;
        function SendRegistro_de_UsuarioData(callBack) {


            var arr = [];
            $("form#CreateRegistro_de_Usuario").find(':input').each(function (index) {
                if ($(this).prop('disabled') == true || $(this).prop('disabled') == "disabled") {
                    arr.push($(this).attr('id'));
                    $(this).prop('disabled', false);
                }
            });

            var formData = new FormData($("form#CreateRegistro_de_Usuario")[0]);
            $.each(arr, function (key, value) {
                $('#' + value).prop('disabled', true);
            });
            var id = $("#ReferenceClave").val();
            var isNew = $('#hdnIsNew').val();

            if (Tab_Datos_Generales && !checkClientValidate('Registro_de_UsuarioCreateForm', 'Datos_Generales')) {
                return false;
            }

            if (Tab_Datos_para_Iniciar_Sesion && !checkClientValidate('Registro_de_UsuarioCreateForm', 'Datos_para_Iniciar_Sesion')) {
                return false;
            }

            if (Tab_Datos_para_validar_tu_cuenta && !checkClientValidate('Registro_de_UsuarioCreateForm', 'Datos_para_validar_tu_cuenta')) {
                return false;
            }

            if (Tab_Validacion && !checkClientValidate('Registro_de_UsuarioCreateForm', 'Validacion')) {
                return false;
            }

            if (Tab_Logica && !checkClientValidate('Registro_de_UsuarioCreateForm', 'Logica')) {
                return false;
            }



            if (isNew == "True") {
                id = CreateID();
                NuevoIDV = id;
            }


            if (Tab_Datos_Generales)
                Save_Datos_Generales(id);

            if (Tab_Datos_para_Iniciar_Sesion)
                Save_Datos_para_Iniciar_Sesion(id);

            if (Tab_Datos_para_validar_tu_cuenta)
                Save_Datos_para_validar_tu_cuenta(id);

            if (Tab_Validacion)
                Save_Validacion(id);

            if (Tab_Logica)
                Save_Logica(id);


            showNotification("@Resources.Resources.SaveSuccess", "success");
            callBack(id);
            return true;
