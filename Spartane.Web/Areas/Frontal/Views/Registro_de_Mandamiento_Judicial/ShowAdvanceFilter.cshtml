<script>
    function InitiateAdvanceSearch() {
        var advanceSearch = {}, hasParameter = false;
        if ($("#txtAdvanceFromClave").val() != "" || $("#txtAdvanceToClave").val() != "") {
            hasParameter = true;
            advanceSearch.Registro_de_Mandamiento_JudicialClaveFrom = $("#txtAdvanceFromClave").val();
            advanceSearch.Registro_de_Mandamiento_JudicialClaveTo = $("#txtAdvanceToClave").val();
        }
        if ($("#txtAdvanceFromFecha_de_Registro").val() != "" || $("#txtAdvanceToFecha_de_Registro").val() != "") {
