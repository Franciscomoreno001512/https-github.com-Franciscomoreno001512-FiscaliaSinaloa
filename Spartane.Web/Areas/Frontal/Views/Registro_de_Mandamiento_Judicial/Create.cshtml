		function Fill_Expediente()
        {
            $.ajax({
                url: "@Url.Content("~/Frontal/Registro_de_Mandamiento_Judicial/Get_Expediente")?Id=" + $("#ReferenceClave").val(),
                type: 'GET',
                dataType: "json",
                async: false,
                success: function (result) {
                                        $('#Clave').val(result.data.Clave);
                    var control = $('#Carpeta_de_Investigacion');
                    control.select2('open');
                    $('.select2-search__field').val(result.data.Carpeta_de_Investigacion).trigger('keyup');
                    control.select2('close');
                    setTimeout(function () {
	                    var data = eval('AutoComplete' + control.selector.replace('#', '') + 'Data');
	                    control.select2({ data: data });
	                    control.val(result.data.Carpeta_de_Investigacion).trigger('change');
                    }, 3000);
                    var control = $('#Causa_Penal');
                    control.select2('open');
                    $('.select2-search__field').val(result.data.Causa_Penal).trigger('keyup');
                    control.select2('close');
                    setTimeout(function () {
	                    var data = eval('AutoComplete' + control.selector.replace('#', '') + 'Data');
	                    control.select2({ data: data });
	                    control.val(result.data.Causa_Penal).trigger('change');
                    }, 3000);
                    Detalle_de_Personas_a_IncluirDataDataTable = result.Personas_a_Incluir;
                    showDetalle_de_Personas_a_IncluirGrid(result.Personas_a_Incluir);
                    $('#Detalle_de_Personas_a_IncluirGrid .inputMoney').inputmask("currency", { prefix: "$", rightAlign: false });
                    $('#Detalle_de_Personas_a_IncluirGrid .gridDatePicker').attr("readonly", "");

                },
                error: function (result) {
                    showNotification("@Resources.Resources.ErrorContactingServer", "error");
                }
            });
        }
		
