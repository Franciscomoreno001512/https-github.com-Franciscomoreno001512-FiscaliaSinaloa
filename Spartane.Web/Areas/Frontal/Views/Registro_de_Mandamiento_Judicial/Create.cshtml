        function Detalle_de_Personas_a_IncluirGetUpdateRowControls(data, nameOfTable, rowIndex) {

            var columnData = [];
            var counter = 0;
            var inputData = "<input type='text' class='fullWidth form-control' />";

            columnData[counter++] = $($($.parseHTML(inputData)).val(data.Persona)).addClass('Persona').attr('id', nameOfTable + 'Persona' + rowIndex).attr('data-field', 'Persona');
              columnData[counter++] = $($($.parseHTML(GetGridCheckBox())).prop('checked', (data.Seleccionar=="true" || data.Seleccionar==true)? 'checked': ''? 'checked': '')).addClass('Seleccionar').attr('id', nameOfTable + 'Seleccionar' + rowIndex).attr('data-field', 'Seleccionar');
            columnData[counter++] = $($($.parseHTML(inputData)).val(data.PersonaId)).addClass('PersonaId inputNumber').attr('id', nameOfTable + 'PersonaId' + rowIndex).attr('data-field', 'PersonaId');

            return columnData;
        }



        function GetFileInfo(fileData, fileUploadData) {

            var $div = $("<div>");
            if (fileUploadData != null && fileUploadData.files.length > 0) {

                $div.append("<label>" + fileUploadData.files[0].name + "</label>");
                //<i class="fa fa-times-circle" id='removeFileAttachment'>
                $div.append($("<i class='fa fa-times-circle' id='removeFileAttachment'>").click(function () {
                    if (confirm(htmlDecode("@Resources.Resources.DeleteConfirm"))) {
                        var parent = $(this).parent().parent();
                        parent.empty();
                        parent.append("<input type='file'>");
                    }
                }));
                return $div;

            } else if (fileUploadData != null) {
                return $.parseHTML(GetFileUploader());
            } else {
                $div.append("<label>" + fileData.FileName + "</label>");
                $div.append($("<i class='fa fa-times-circle' id='removeFileAttachment'>").click(function () {
                    if (confirm(htmlDecode("@Resources.Resources.DeleteConfirm"))) {
                        var parent = $(this).parent().parent();
                        parent.empty();
                        parent.append("<input type='file'>");
                    }
                }));
                return $div;
            }
        }

        var removedDetalle_de_Personas_a_IncluirData = [];
        function Detalle_de_Personas_a_IncluirDeleteRow(rowIndex, row) {
            var rowIndexTable = Detalle_de_Personas_a_IncluirTable.fnGetPosition($(row).parent().parent()[0]);
            var permission = {            
                 Delete: '@permissionDetalle_de_Personas_a_Incluir.Delete' == 'True'
            };
            if (!permission.Delete || isConsultMode)
                return;            
            if (confirm(htmlDecode("@Resources.Resources.DeleteConfirm"))) {
