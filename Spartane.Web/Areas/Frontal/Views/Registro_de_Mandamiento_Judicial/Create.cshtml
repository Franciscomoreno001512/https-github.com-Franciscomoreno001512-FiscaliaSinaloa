        function Detalle_de_Personas_a_IncluirrowCreationGrid(row, data, index) {
            var permission =  {            
                 Edit: '@permissionDetalle_de_Personas_a_Incluir.Edit' == 'True',
                 Delete: '@permissionDetalle_de_Personas_a_Incluir.Delete' == 'True'
            };
            setDetalle_de_Personas_a_IncluirValidation();
            var Detalle_de_Personas_a_IncluirRowElement = "Detalle_de_Personas_a_Incluir_" + index.toString();
            $(row).addClass(Detalle_de_Personas_a_IncluirRowElement);
            var nameOfTable='Detalle_de_Personas_a_Incluir_';
            var rowIndex = '_' + index;
            Detalle_de_Personas_a_IncluirinsertRowCurrentIndex = index;

            row.innerHTML = "";

            if (data.IsInsertRow) {

                if (row.childNodes.length != 0) {
                    for (var i = 0; i < row.childNodes.length; i++) {
                        row.childNodes[i].remove();
                    }
                }
                var controls = GetInsertDetalle_de_Personas_a_IncluirRowControls(index);
                var actionColInsert = $('<td>');

                var abc = "if(dynamicFieldValidation('" + Detalle_de_Personas_a_IncluirRowElement + "')){ Detalle_de_Personas_a_IncluirInsertRow(" + index + "); }";
                var insertRowHTML = '<a  data-toggle="tooltip" title="Confirmar Registro" onclick="' + abc + '">';
                $('<i class="fa fa-check">').appendTo($(insertRowHTML).appendTo(actionColInsert));
				$('<i class="fa fa-times">').appendTo($("<a data-toggle='tooltip' title='Cancelar Registro' onclick='Detalle_de_Personas_a_IncluirCancelRow(" + index + ")'>").appendTo(actionColInsert));
                actionColInsert.appendTo(row);

                for (i = 0; i < controls.length; i++) {
					var idHeader = $(controls[i]).data('field') + 'Header';
					if($(controls[i]).length > 1){
						idHeader = $($(controls[i])[1]).data('field') + 'Header';
					}					

					var classe = ($('#Detalle_de_Personas_a_IncluirGrid .' + idHeader).hasClass('dt-right') ? "dt-right" : "") + $('#Detalle_de_Personas_a_IncluirGrid .' + idHeader ).css('display') == 'none' ? " hide" : "";
				   
					if ($(controls[i]).next().length > 0) {
					    var div = $(controls[i]).next();
					    $('<td class="' + classe + '">').append($(controls[i])).append(div).appendTo(row);
					}else
					    $(controls[i]).appendTo($('<td class="' + classe + '">').appendTo(row));
					
                }
                $('#Detalle_de_Personas_a_IncluirGrid .inputMoney').inputmask("currency", { prefix: "$", rightAlign: false });
                $('#Detalle_de_Personas_a_IncluirGrid .gridDatePicker').attr("readonly", "");
                return;
            }

            var actionCol = $('<td>');
			var edit = "<a title='@Resources.Resources.Edit' class='edit-MR' onclick='Detalle_de_Personas_a_IncluirEditRow(" + index + ", this)'>";
            if (!permission.Edit || isConsultMode)
                edit = "";
            var editPopUp = "<a title='@Resources.Resources.Edit' class='edit-popup-MR' data-toggle='modal' data-target='#AddDetalle_de_Personas_a_Incluir-form' onclick='Detalle_de_Personas_a_IncluirEditRowPopup(" + index + ", this)'>";
            if (!permission.Edit || isConsultMode)
                editPopUp = "";
            var eliminar = "<a title='@Resources.Resources.Delete' class='eliminar-MR' onclick='Detalle_de_Personas_a_IncluirDeleteRow(" + index + ", this)'>";
            if (!permission.Delete || isConsultMode)
                eliminar = "";
            $('<i class="fa fa-pencil">').appendTo($(edit).appendTo(actionCol));
