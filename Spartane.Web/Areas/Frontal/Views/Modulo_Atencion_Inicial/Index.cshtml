        var Unidad =  $($('table td .filterData.form-control:input')[counter++]).val();
        var Municipio =  $($('table td .filterData.form-control:input')[counter++]).val();
        var Region =  $($('table td .filterData.form-control:input')[counter++]).val();
        var Tipo_de_Denuncia =  $($('table td .filterData.form-control:input')[counter++]).val();
        var NUAT =  $($('table td .filterData.form-control:input')[counter++]).val();
        var CDI =  $($('table td .filterData.form-control:input')[counter++]).val();
        var Expedientes_Relacionados =  $($('table td .filterData.form-control:input')[counter++]).val();
        var Estatus =  $($('table td .filterData.form-control:input')[counter++]).val();
        var Generar_Cita = $($('table td .filterData.form-control:input')[counter++]).val();
