		function Save_Datos_Generales(newId) {
			var data = new FormData();
			var estatus = $('#Estatus_de_Turno').val();
            if (finalizaTurno == 1) { estatus = 3; }
			var data = new FormData();
			data.append('Folio', newId);
			data.append('Fecha_de_Turno', $('#Fecha_de_Turno').val());
			data.append('Fecha_de_Turno', $('#Fecha_de_Turno').val());
                data.append('Hora_de_Turno', $('#Hora_de_Turno').val());
                data.append('Numero_de_Turno', $('#Numero_de_Turno').val());
                data.append('Unidad_de_Atencion', $('#Unidad_de_Atencion').val());
                data.append('Recepcion', $('#Recepcion').val());
                data.append('Nombres', $('#Nombres').val());
                data.append('Apellido_Paterno', $('#Apellido_Paterno').val());
                data.append('Apellido_Materno', $('#Apellido_Materno').val());
                data.append('Nombre_Completo', $('#Nombre_Completo').val());
                data.append('Sexo', $('#Sexo').val());
                data.append('Edad', $('#Edad').val());
                data.append('Tipo_de_Atencion', $('#Tipo_de_Atencion').val());
                data.append('Tipo_de_Identificacion', $('#Tipo_de_Identificacion').val());
                data.append('Otra_Identificacion', $('#Otra_Identificacion').val());
                data.append('Numero_de_Identificacion', $('#Numero_de_Identificacion').val());
                data.append('Urgencia', $('#Urgencia').prop('checked'));
                data.append('Tipo_de_Urgencia', $('#Tipo_de_Urgencia').val());
                data.append('Orientador', $('#Orientador').val());
                data.append('Estatus_de_Turno', estatus);
                data.append('Modulo', $('#Modulo').val());
                data.append('Motivo_Finalizacion_Turno', $('#Motivo_Finalizacion_Turno').val());
                data.append('Observaciones', $('#Observaciones').val());
                data.append('Fecha_de_Asignacion', $('#Fecha_de_Asignacion').val());
                data.append('Hora_de_Asignacion', $('#Hora_de_Asignacion').val());
                data.append('Fecha_de_Finalizacion', $('#Fecha_de_Finalizacion').val());
                data.append('Hora_de_Finalizacion', $('#Hora_de_Finalizacion').val());

			$.ajax({
				url: '@Url.Content("~/Frontal/Asignacion_de_Turnos/Post_Datos_Generales")',
				type: 'POST',
				data: data,
				async: false,
				dataType: "json",
				contentType: false,
				processData: false,
				success: function (result) {
					if (result < 0) {
						showNotification("@Resources.Resources.ErrorSave", "Error al guardar Datos_Generales");
					} else {
						newId = result;
						if (finalizaTurno == 1) {
                            ActualizrFechasFinalizar(newId);
                        }                    
					}
				},
				error: function (result) {
					showNotification("@Resources.Resources.ErrorContactingServer", "Error al guardar Datos_Generales");
				},
				cache: false
			});
		}


