        function SendAsignacion_de_TurnosData(callBack) {
			
					
		 var arr = []; 
		 $("form#CreateAsignacion_de_Turnos").find(':input').each(function( index ) {		 
			if ($(this).prop('disabled')==true|| $(this).prop('disabled')=="disabled")
			{		
				arr.push($(this).attr('id')); 
				$(this).prop('disabled', false);  	
			}
	    });
		
             var formData = new FormData($("form#CreateAsignacion_de_Turnos")[0]);
			$.each( arr, function( key, value ) {
			  $('#' +value).prop('disabled', true);
			});
