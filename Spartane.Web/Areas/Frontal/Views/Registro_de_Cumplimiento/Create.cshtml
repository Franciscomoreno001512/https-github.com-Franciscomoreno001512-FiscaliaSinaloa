                    control.select2('open');
                    $('.select2-search__field').val(result.data.Poblacion).trigger('keyup');
                    control.select2('close');
                    setTimeout(function () {
	                    var data = eval('AutoComplete' + control.selector.replace('#', '') + 'Data');
	                    control.select2({ data: data });
	                    control.val(result.data.Poblacion).trigger('change');
                    }, 3000);
                    var control = $('#Colonia');
                    control.select2('open');
                    $('.select2-search__field').val(result.data.Colonia).trigger('keyup');
                    control.select2('close');
                    setTimeout(function () {
	                    var data = eval('AutoComplete' + control.selector.replace('#', '') + 'Data');
	                    control.select2({ data: data });
	                    control.val(result.data.Colonia).trigger('change');
                    }, 3000);
                    $('#Observaciones_del_Cumplimiento').val(result.data.Observaciones_del_Cumplimiento);
                    Personas_donde_se_ejecuto_mandamientoDataDataTable = result.Personas_donde_se_ejecuto_mandamiento;
                    showPersonas_donde_se_ejecuto_mandamientoGrid(result.Personas_donde_se_ejecuto_mandamiento);
                    $('#Personas_donde_se_ejecuto_mandamientoGrid .inputMoney').inputmask("currency", { prefix: "$", rightAlign: false });
                    $('#Personas_donde_se_ejecuto_mandamientoGrid .gridDatePicker').attr("readonly", "");

                },
                error: function (result) {
                    showNotification("@Resources.Resources.ErrorContactingServer", "error");
                }
            });
        }
		
		function Save_Datos_Generales(newId) {
			var data = new FormData();
			data.append('Clave', newId);
			                data.append('Mandamiento_Judicial', $('#Mandamiento_Judicial').val());
                data.append('Fecha_de_Registro', $('#Fecha_de_Registro').val());
                data.append('Hora_de_Registro', $('#Hora_de_Registro').val());
                data.append('Usuario_de_Registro', $('#Usuario_de_Registro').val());
                data.append('Resolucion', $('#Resolucion').val());
                data.append('Fecha_de_Resolucion', $('#Fecha_de_Resolucion').val());
                data.append('Numero_de_Oficio', $('#Numero_de_Oficio').val());
                data.append('Radiograma', $('#Radiograma').val());
                data.append('Partida_que_cumplimenta', $('#Partida_que_cumplimenta').val());
                data.append('Area_que_envia_cumplimiento', $('#Area_que_envia_cumplimiento').val());
                data.append('Estado', $('#Estado').val());
                data.append('Municipio', $('#Municipio').val());
                data.append('Poblacion', $('#Poblacion').val());
                data.append('Colonia', $('#Colonia').val());
                data.append('Observaciones_del_Cumplimiento', $('#Observaciones_del_Cumplimiento').val());

			$.ajax({
				url: '@Url.Content("~/Frontal/Registro_de_Cumplimiento/Post_Datos_Generales")',
				type: 'POST',
				data: data,
				async: false,
				dataType: "json",
				contentType: false,
				processData: false,
				success: function (result) {
					if (result < 0) {
						showNotification("@Resources.Resources.ErrorSave", "Error al guardar Datos_Generales");
					} else {
						newId = result;
                    					    SendPersonas_donde_se_ejecuto_mandamiento(null, newId);

					}
				},
				error: function (result) {
					showNotification("@Resources.Resources.ErrorContactingServer", "Error al guardar Datos_Generales");
				},
				cache: false
			});
		}


    </script>
