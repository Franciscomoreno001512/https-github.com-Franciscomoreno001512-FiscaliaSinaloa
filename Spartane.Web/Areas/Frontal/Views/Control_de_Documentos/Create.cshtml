                }

                if (txpiepagina != "") {
                    TXTextControl.headersAndFooters.add(TXTextControl.HeaderFooterType.Footer, callback1F, errorCallback1);
                    b_piep = true;
                }
            }

            function autoSaveDocument() {
                console.log("Entro autoSaveDocument");
                // store the document in the session storage
                // which is shared over all browser tabs (complete session)
                var save1 = new TXTextControl.SaveSettings();
                TXTextControl.save(TXTextControl.StreamType.HTMLFormat, callbacksave, save1, errorCallback1);
                if (b_encabezado)
                    headerprincipal.selection.save(TXTextControl.StreamType.HTMLFormat, callbacksaveE, save1, errorCallback1);
                 else
                    TXTextControl.headersAndFooters.getItem(TXTextControl.HeaderFooterType.Header, callback2save, errorCallback1);
                if (b_piep)
                    footerprincipal.selection.save(TXTextControl.StreamType.HTMLFormat, callbacksaveF, save1, errorCallback1);
                 else
                    TXTextControl.headersAndFooters.getItem(TXTextControl.HeaderFooterType.Footer, callback3save, errorCallback1);
                TXTextControl.saveDocument(TXTextControl.StreamType.AdobePDF, callbacksaveDoc, save1, errorCallback1);

                TXTextControl.saveDocument(
                    TXTextControl.streamType.InternalUnicodeFormat, function (e) {

                        sessionStorage.document = e.data;
                    }
                );
            }

            function restoreDocument() {
                console.log("Entro restoreDocument");


                // if document exists, load it from the session storage
                if (sessionStorage["document"]) {

                    $("#txReconnectMsg").show(); // hide dialog

                    TXTextControl.loadDocument(
                        TXTextControl.streamType.InternalUnicodeFormat,
                        sessionStorage.document);
                }
                else
                    $("#txReconnectMsg").hide(); // hide dialog
            }

            function callback1(header) {
                TXTextControl.headersAndFooters.getItem(TXTextControl.HeaderFooterType.Header, callback2, errorCallback1)
            }
