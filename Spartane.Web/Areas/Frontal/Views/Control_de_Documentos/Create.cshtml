            function TXTextControlLoad() {
                console.log("Entro TXTextControlLoad");

                var encoded = btoa(cuerpodocumento);  // btoa base-64-encodes strings.
                TXTextControl.loadDocument(TXTextControl.StreamType.HTMLFormat, encoded);
                debugger;
                if (txencabezado != "") {
                    TXTextControl.headersAndFooters.add(TXTextControl.HeaderFooterType.Header, callback1, errorCallback1);
