                data.append('Numero_Interior_del_Tutor', $('#Numero_Interior_del_Tutor').val());
                data.append('Referencia', $('#Referencia').val());
                data.append('Latitud_Tutor', $('#Latitud_Tutor').val());
                data.append('Longitud_Tutor', $('#Longitud_Tutor').val());
                data.append('Telefono_del_Tutor', $('#Telefono_del_Tutor').val());
                data.append('Extension_del_Tutor', $('#Extension_del_Tutor').val());
                data.append('Celular_del_Tutor', $('#Celular_del_Tutor').val());
                data.append('Correo_Electronico_del_Tutor', $('#Correo_Electronico_del_Tutor').val());

			$.ajax({
				url: '@Url.Content("~/Frontal/Detalle_de_Imputado/Post_Datos_del_Tutor")',
				type: 'POST',
				data: data,
				async: false,
				dataType: "json",
				contentType: false,
				processData: false,
				success: function (result) {
					if (result < 0) {
						showNotification("@Resources.Resources.ErrorSave", "Error al guardar Datos_del_Tutor");
					} else {
						newId = result;
                    
					}
				},
				error: function (result) {
					showNotification("@Resources.Resources.ErrorContactingServer", "Error al guardar Datos_del_Tutor");
				},
				cache: false
			});
		}


    </script>
