<script type="text/javascript">


   function GetCatalogPopup(catalog, nameAttribute, isMR, nameMR) {
        if (!isMR)
            var url = "@Url.Action("Create", "Catalog", new { Module_Id = "1", isPartial = true, nameAttribute = "AttributoNombre" })";
        else
            var url = "@Url.Action("Create", "Catalog", new { Module_Id = "1", isPartial = true, isMR = true, nameMR = "Multirenglon",nameAttribute= "AttributoNombre" })";

        url = url.replace("Catalog", catalog).replace("Multirenglon", nameMR).replace("AttributoNombre", nameAttribute);

        window.open(htmlDecode(url), '_blank', 'left=200,top=100,width=850,height=550,toolbar=0,resizable=0');
    }

    function RefreshCatalog(catalog, control) {

	 if ($('#' + control).hasClass('AutoComplete'))   {
		   if ($('#' + control).attr("data-isfilter") == "true") {
				eval(GetReglaFilter(control, $('#ObjectId').val()));
				return;
			}
		}
			
			var url = '@Url.Content("~/Frontal/Solicitud_de_Servicios_de_Apoyo/GetCatalogAll")';
			url = url.replace(/Catalog/g, catalog);

			$.ajax({
				url: url,
				async: true,
				success:function(result)
				{
					addItemsToSelect($('#' + control), result);
				},
				error: function (result)
				{
					showNotification("@Resources.Resources.ErrorContactingServer", "error");
				}
			});
		
    }


        function SendSolicitud_de_Servicios_de_ApoyoData(callBack) {
			
					
		 var arr = []; 
		 $("form#CreateSolicitud_de_Servicios_de_Apoyo").find(':input').each(function( index ) {		 
			if ($(this).prop('disabled')==true|| $(this).prop('disabled')=="disabled")
			{		
				arr.push($(this).attr('id')); 
				$(this).prop('disabled', false);  	
			}
	    });
		
             var formData = new FormData($("form#CreateSolicitud_de_Servicios_de_Apoyo")[0]);
			$.each( arr, function( key, value ) {
			  $('#' +value).prop('disabled', true);
			});
			var id = $("#ReferenceFolio").val();
            var isNew = $('#hdnIsNew').val();
           
			if (Tab_Datos_Generales && !checkClientValidate('Solicitud_de_Servicios_de_ApoyoCreateForm', 'Datos_Generales')) {
				return false;
			}						

			if (Tab_Recepcion && !checkClientValidate('Solicitud_de_Servicios_de_ApoyoCreateForm', 'Recepcion')) {
				return false;
			}						

			if (Tab_Asignacion && !checkClientValidate('Solicitud_de_Servicios_de_ApoyoCreateForm', 'Asignacion')) {
				return false;
			}						

			if (Tab_Contestacion && !checkClientValidate('Solicitud_de_Servicios_de_ApoyoCreateForm', 'Contestacion')) {
				return false;
			}						

			if (Tab_Autorizacion && !checkClientValidate('Solicitud_de_Servicios_de_ApoyoCreateForm', 'Autorizacion')) {
				return false;
			}						


			
			if (isNew == "True")
            {
                id = CreateID();
            }
			
			
			if (Tab_Datos_Generales)
				Save_Datos_Generales(id);

			if (Tab_Recepcion)
				Save_Recepcion(id);

			if (Tab_Asignacion)
				Save_Asignacion(id);

			if (Tab_Contestacion)
				Save_Contestacion(id);

			if (Tab_Autorizacion)
				Save_Autorizacion(id);


			showNotification("@Resources.Resources.SaveSuccess", "success");
			callBack(id);
            return true;
        };
		function CreateID()
		{
			var newId = 0;
			$.ajax({
				url: '@Url.Content("~/Frontal/Solicitud_de_Servicios_de_Apoyo/CreateID")',
				type: 'GET',
				async: false,
				success: function (result) {
					if (result < 0) {
						showNotification("@Resources.Resources.ErrorSave", "error");
					} else {
						newId = result;
					}
				},
				error: function (result) {
					showNotification("@Resources.Resources.ErrorContactingServer", "error");
				},
				cache: false,
				contentType: false,
				processData: false
			});
			return newId;
		}
		
