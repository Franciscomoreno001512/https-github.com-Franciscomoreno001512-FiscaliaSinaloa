                success: function (result) {
                    Agravantes_del_DelitoDataDataTable = result.data;
                    showAgravantes_del_DelitoGrid(result.data);
                    $('#Agravantes_del_DelitoGrid .inputMoney').inputmask("currency", { prefix: "$", rightAlign: false });
                    $('#Agravantes_del_DelitoGrid .gridDatePicker').attr("readonly", "");
                },
                error: function (result) {
                    showNotification("@Resources.Resources.ErrorContactingServer", "error");
                }
            });
        }
        function Agravantes_del_DelitoGetUpdateRowControls(data, nameOfTable, rowIndex) {

            var columnData = [];
            var counter = 0;
            var inputData = "<input type='text' class='fullWidth form-control' />";

           columnData[counter++] = $($(GetAgravantes_del_Delito_AgravantesDropDown()).val(data.Agravante)).addClass('Agravante inputNumber').attr('id', nameOfTable + 'Agravante' + rowIndex).attr('data-field', 'Agravante').after($.parseHTML(addNew('Agravantes_del_Delito', 'Agravantes','Agravante', 265686)));

            return columnData;
        }



        function GetFileInfo(fileData, fileUploadData) {

            var $div = $("<div>");
            if (fileUploadData != null && fileUploadData.files.length > 0) {

                $div.append("<label>" + fileUploadData.files[0].name + "</label>");
                //<i class="fa fa-times-circle" id='removeFileAttachment'>
                $div.append($("<i class='fa fa-times-circle' id='removeFileAttachment'>").click(function () {
                    if (confirm(htmlDecode("@Resources.Resources.DeleteConfirm"))) {
                        var parent = $(this).parent().parent();
                        parent.empty();
                        parent.append("<input type='file'>");
                    }
                }));
                return $div;

            } else if (fileUploadData != null) {
                return $.parseHTML(GetFileUploader());
            } else {
                $div.append("<label>" + fileData.FileName + "</label>");
                $div.append($("<i class='fa fa-times-circle' id='removeFileAttachment'>").click(function () {
                    if (confirm(htmlDecode("@Resources.Resources.DeleteConfirm"))) {
                        var parent = $(this).parent().parent();
                        parent.empty();
                        parent.append("<input type='file'>");
                    }
                }));
                return $div;
            }
        }

        var removedAgravantes_del_DelitoData = [];
        function Agravantes_del_DelitoDeleteRow(rowIndex, row) {
            var rowIndexTable = Agravantes_del_DelitoTable.fnGetPosition($(row).parent().parent()[0]);
            var permission = {            
                 Delete: '@permissionAgravantes_del_Delito.Delete' == 'True'
            };
            if (!permission.Delete || isConsultMode)
                return;            
            if (confirm(htmlDecode("@Resources.Resources.DeleteConfirm"))) {
              if (EjecutarValidacionesAlEliminarMRAgravantes_del_Delito('Agravantes_del_Delito_', '_' + rowIndex)) {
                var prevData = Agravantes_del_DelitoTable.fnGetData(rowIndexTable );
                removedAgravantes_del_DelitoData.push(prevData);
                Agravantes_del_DelitoTable.fnDeleteRow(rowIndexTable );
				showAgravantes_del_DelitoGrid(Agravantes_del_DelitoTable.fnGetData());
              }
            }
			initiateUIControls();
        }

        function Agravantes_del_DelitorowCreationGrid(row, data, index) {
            var permission =  {            
                 Edit: '@permissionAgravantes_del_Delito.Edit' == 'True',
                 Delete: '@permissionAgravantes_del_Delito.Delete' == 'True'
            };
            setAgravantes_del_DelitoValidation();
            var Agravantes_del_DelitoRowElement = "Agravantes_del_Delito_" + index.toString();
            $(row).addClass(Agravantes_del_DelitoRowElement);
            var nameOfTable='Agravantes_del_Delito_';
            var rowIndex = '_' + index;
            Agravantes_del_DelitoinsertRowCurrentIndex = index;

            row.innerHTML = "";

            if (data.IsInsertRow) {

                if (row.childNodes.length != 0) {
                    for (var i = 0; i < row.childNodes.length; i++) {
                        row.childNodes[i].remove();
                    }
                }
                var controls = GetInsertAgravantes_del_DelitoRowControls(index);
                var actionColInsert = $('<td>');

                var abc = "if(dynamicFieldValidation('" + Agravantes_del_DelitoRowElement + "')){ Agravantes_del_DelitoInsertRow(" + index + "); }";
                var insertRowHTML = '<a  data-toggle="tooltip" title="Confirmar Registro" onclick="' + abc + '">';
                $('<i class="fa fa-check">').appendTo($(insertRowHTML).appendTo(actionColInsert));
				$('<i class="fa fa-times">').appendTo($("<a data-toggle='tooltip' title='Cancelar Registro' onclick='Agravantes_del_DelitoCancelRow(" + index + ")'>").appendTo(actionColInsert));
                actionColInsert.appendTo(row);

                for (i = 0; i < controls.length; i++) {
					var idHeader = $(controls[i]).data('field') + 'Header';
					if($(controls[i]).length > 1){
						idHeader = $($(controls[i])[1]).data('field') + 'Header';
					}					

					var classe = ($('#Agravantes_del_DelitoGrid .' + idHeader).hasClass('dt-right') ? "dt-right" : "") + $('#Agravantes_del_DelitoGrid .' + idHeader ).css('display') == 'none' ? " hide" : "";
				   
					if ($(controls[i]).next().length > 0) {
					    var div = $(controls[i]).next();
					    $('<td class="' + classe + '">').append($(controls[i])).append(div).appendTo(row);
					}else
					    $(controls[i]).appendTo($('<td class="' + classe + '">').appendTo(row));
					
                }
                $('#Agravantes_del_DelitoGrid .inputMoney').inputmask("currency", { prefix: "$", rightAlign: false });
                $('#Agravantes_del_DelitoGrid .gridDatePicker').attr("readonly", "");
                return;
            }

            var actionCol = $('<td>');
			var edit = "<a title='@Resources.Resources.Edit' class='edit-MR' onclick='Agravantes_del_DelitoEditRow(" + index + ", this)'>";
            if (!permission.Edit || isConsultMode)
                edit = "";
            var editPopUp = "<a title='@Resources.Resources.Edit' class='edit-popup-MR' data-toggle='modal' data-target='#AddAgravantes_del_Delito-form' onclick='Agravantes_del_DelitoEditRowPopup(" + index + ", this)'>";
            if (!permission.Edit || isConsultMode)
                editPopUp = "";
            var eliminar = "<a title='@Resources.Resources.Delete' class='eliminar-MR' onclick='Agravantes_del_DelitoDeleteRow(" + index + ", this)'>";
            if (!permission.Delete || isConsultMode)
                eliminar = "";
            $('<i class="fa fa-pencil">').appendTo($(edit).appendTo(actionCol));
            //$('<i class="fa fa-edit">').appendTo($(editPopUp).appendTo(actionCol));
            $('<i class="fa fa-trash-o">').appendTo($(eliminar).appendTo(actionCol));
			if ($('.Action' + 'Header').css('display') != 'none')
				actionCol.appendTo(row);

            if ($('.Agravante' + 'Header').css('display') != 'none')
                $('<td id="'+nameOfTable+'Agravante'+rowIndex+'">').text(GetAgravantes_del_Delito_AgravantesName(data.Agravante)).appendTo(row);
	    else 
		$('<td style="display:none;" id="'+nameOfTable+'Agravante'+rowIndex+'">').text(GetAgravantes_del_Delito_AgravantesName(data.Agravante)).appendTo(row);

        }

