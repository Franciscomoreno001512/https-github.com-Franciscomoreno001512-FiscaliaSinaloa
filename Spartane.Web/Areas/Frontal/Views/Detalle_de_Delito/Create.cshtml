                    $('#Ruta').val(result.data.Ruta);
                    $('#Estado_del_Conductor').prop('checked', result.data.Estado_del_Conductor);

                },
                error: function (result) {
                    showNotification("@Resources.Resources.ErrorContactingServer", "error");
                }
            });
        }
		
