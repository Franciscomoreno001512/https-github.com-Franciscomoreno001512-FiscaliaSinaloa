        var Agravantes_del_DelitoinsertRowCurrentIndex = -1;
        var Agravantes_del_DelitoDataDataTable;
        function getAgravantes_del_DelitoData() {
            $.ajax({
                url: "@Url.Content("~/Frontal/Detalle_de_Delito/GetAgravantes_del_Delito")" + "?draw=1&start=1&length=2147483647" + ($("#ReferenceClave").val() != "" ? "&RelationId=" + $("#ReferenceClave").val() : ""),
                type: 'GET',
